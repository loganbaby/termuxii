    "*",
    "**/*"
  ],
  "C_Cpp_Runner.excludeSearch": [
    "**/build",
    "**/build/**",
    "**/.*",
    "**/.*/**",
    "**/.vscode",
    "**/.vscode/**"
  ],
  "files.associations": {
    "exception": "cpp",
    "iostream": "cpp",
    "ostream": "cpp"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, ColumnLimit: 140}",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, ColumnLimit: 140}",
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "html.format.wrapLineLength": 300,
  "editor.autoIndent": "none",
